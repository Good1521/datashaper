{
	"$schema": "../../../../../../schema/workflow.json",
	"name": "Sparkline",
	"description": "Aggregates a daily prices into an array to demonstrate Sparkline cells",
	"steps": [
		{
			"verb": "aggregate",
			"input": "data/stocks.csv",
			"output": "sparkline",
			"args": {
				"groupby": "Symbol",
				"column": "Close",
				"operation": "array_agg",
				"to": "Daily"
			}
		}
	]
}
