{
	"name": "Binning",
	"description": "Shows how different binning strategies and clamping work.",
	"steps": [
		{
			"id": "bin-1",
			"verb": "bin",
			"inputs": { "default": "data/products.csv" },
			"args": {
				"column": "FY21 Sales",
				"strategy": "auto",
				"to": "Binned"
			}
		},
		{
			"id": "bin-2",
			"verb": "bin",
			"inputs": { "default": "data/products.csv" },
			"args": {
				"column": "FY21 Sales",
				"strategy": "fixed width",
				"to": "Binned",
				"fixedwidth": 100000
			}
		},
		{
			"id": "bin-3",
			"verb": "bin",
			"inputs": { "default": "data/products.csv" },
			"args": {
				"column": "FY21 Sales",
				"strategy": "fixed count",
				"to": "Binned",
				"fixedcount": 100
			}
		},
		{
			"id": "bin-4",
			"verb": "bin",
			"inputs": { "default": "data/products.csv" },
			"args": {
				"column": "FY21 Sales",
				"strategy": "fixed width",
				"to": "Binned",
				"fixedwidth": 100000,
				"min": 20000,
				"max": 1000000
			}
		},
		{
			"id": "bin-5",
			"verb": "bin",
			"inputs": { "default": "data/products.csv" },
			"args": {
				"column": "FY21 Sales",
				"strategy": "fixed width",
				"to": "Binned",
				"fixedwidth": 100000,
				"min": 20000,
				"max": 1000000,
				"clamped": true
			}
		}
	]
}
