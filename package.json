{
	"name": "@datashaper/project",
	"private": true,
	"scripts": {
		"_release_packages": "turbo run release",
		"_cut_schema": "yarn node scripts/release-schema.mjs",
		"_version_cut": "yarn version apply --all",
		"_copy_versions": "yarn node scripts/copy-schemas.mjs",
		"generate:versions": "yarn node scripts/create_versions_file.mjs",
		"clean": "turbo run clean --parallel",
		"build": "turbo run build",
		"bundle": "turbo run bundle",
		"lint": "turbo run lint",
		"test": "turbo run test",
		"clean:": "yarn clean",
		"build:": "yarn build",
		"bundle:": "yarn bundle",
		"lint:": "yarn lint",
		"test:": "yarn test",
		"start": "SB_QUIET=true turbo run start --parallel",
		"deploy": "turbo run deploy",
		"prettify": "essex prettify",
		"ci": "turbo run ci",
		"is_clean": "git diff-index HEAD --quiet || git status",
		"release": "run-s clean _version_cut ci _release_packages _cut_schema _copy_versions",
		"update_sdks": "yarn dlx @yarnpkg/sdks vscode"
	},
	"devDependencies": {
		"@essex/eslint-config": "^20.4.0",
		"@essex/eslint-plugin": "^20.4.0",
		"@essex/jest-config": "^21.0.18",
		"@essex/prettier-config": "^18.0.5",
		"@essex/scripts": "^22.2.1",
		"@types/jest": "^29.2.5",
		"@types/node": "^18.11.18",
		"@types/semver": "^7.3.13",
		"arquero": "^5.1.0",
		"eslint": "^8.31.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.8.2",
		"semver": "^7.3.8",
		"turbo": "^1.6.3",
		"typescript": "^4.9.4"
	},
	"workspaces": [
		"javascript/*"
	],
	"packageManager": "yarn@3.3.1"
}
